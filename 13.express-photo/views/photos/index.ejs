<!doctype html>
<html>
  <head>
    <title><%= title %></title>
    <link href="/stylesheets/style.css" rel="stylesheets">
  </head>
  <body>
    <h1>Photos</h1>
    <p>Express stock photo application.</p>
    <div id="photos">
      <% photos.forEach(function (photo) { %>
        <div class="photo">
          <h2><%=photo.name%></h2>
          <img src="<%=photo.path%>" width=100 height=100>
        </div>
      <% }) %>
    </div>
  </body>
</html>
